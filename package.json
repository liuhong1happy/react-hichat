{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "server": "node ./server/bin/www",
    "start": "watchify -o server/public/javascripts/index.js -v -d src/js/app.js",
    "build": "browserify src/js/app.js -t [envify --NODE_ENV production] | uglifyjs -cm > server/public/javascripts/index.min.js",
    "less": "lessc src/less/index.less  server/public/stylesheets/index.css"
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ejs": "^2.4.2",
    "express": "~4.13.4",
    "jade": "~1.11.0",
    "mongoose": "^4.5.5",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.0"
  }
}
